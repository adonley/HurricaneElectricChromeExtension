{
  "name": "Hurricane Electric Quicklinks",
  "manifest_version": 2,
  "version": "1.0",
  "description": "Quicklinks to all of Hurricane Electrics Management Systems",
  "homepage_url": "http://he.net",
  "permissions": [
    "tabs"
  ],

  "icons": { "16" : "images/16.png",
             "48" : "images/48.png",
            "128" : "images/128.png"},

  "browser_action": {
    "default_icon": "images/icon.png",
    "default_title": "Hurricane Electric's Quicklinks",
    "default_popup": "popup.html"
  }
}